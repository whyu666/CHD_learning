#include <iostream>
using namespace std;
int main()
{
	int n,m,i,j;
	cin>>n>>m;
	int a[n+20][m+20];
	char c;
	for(i=0;i<=n;i++)
	{
		for(j=0;j<=m;j++)
		{
			a[i][j]=-2;
		}
	}
	for(i=1;i<=n;i++)
	{
		for(j=1;j<=m;j++)
		{
			cin>>c;
			if(c=='*')
			{
				a[i][j]=-1;
			}
			else
			{
				a[i][j]=0;
			}
		}
	}
	for(i=1;i<=n;i++)
	{
		for(j=1;j<=m;j++)
		{
			if(a[i][j]!=-1)
			{
				if(a[i-1][j-1]==-1)
				{
					a[i][j]++;
				}
				if(a[i-1][j]==-1)
				{
					a[i][j]++;
				}
				if(a[i-1][j+1]==-1)
				{
					a[i][j]++;
				}
				if(a[i][j-1]==-1)
				{
					a[i][j]++;
				}
				if(a[i][j]==-1)
				{
					a[i][j]++;
				}
				if(a[i][j+1]==-1)
				{
					a[i][j]++;
				}
				if(a[i+1][j-1]==-1)
				{
					a[i][j]++;
				}
				if(a[i+1][j]==-1)
				{
					a[i][j]++;
				}
				if(a[i+1][j+1]==-1)
				{
					a[i][j]++;
				}
			}
		}
	}
	for(i=1;i<=n;i++)
	{
		for(j=1;j<=m;j++)
		{
			if(a[i][j]==-1)
			{
				cout<<"*";
			}
			else
			{
				cout<<a[i][j];
			}
		}
		cout<<endl;
	}
	return 0;
}
