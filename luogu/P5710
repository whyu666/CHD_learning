#include <iostream>
using namespace std;
int main() 
{
	int x;
	bool a=false,b=false;
	cin>>x;
	if(x%2==0)
	{
		a=true;
	}
	if(x>4&&x<=12)
	{
		b=true;
	}
	if(a==true&&b==true)
	{
		cout<<"1 ";
	}
	else
	{
		cout<<"0 ";
	}
	if(a==true|b==true)
	{
		cout<<"1 ";
	}
	else
	{
		cout<<"0 ";
	}
	if((a==true&&b==false)|(a==false&&b==true))
	{
		cout<<"1 ";
	}
	else
	{
		cout<<"0 ";
	}
	if(a==false&&b==false)
	{
		cout<<"1 ";
	}
		else
	{
		cout<<"0 ";
	}
	return 0;
}
